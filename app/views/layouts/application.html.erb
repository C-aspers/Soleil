              </li>
              <li class="nav-item active">
                <a class="nav-link" href="/users">Dashboard</a>
              </li>
              <li class="nav-item active">
                <a class="nav-link" <%=link_to "Sign out", destroy_user_session_path, method: :delete%></a>
              </li>
            </ul>
          </div>
        </div>
      </nav>
    
   

    <%= yield %>
